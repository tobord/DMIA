    "import numpy as np\n",
    "w, X, y = np.random.random(4), np.random.random((5, 4)), 2*(np.random.randint(0, 2, 5)-0.5)\n",
    "import scipy as sp\n",
    "from scipy import special\n",
    "\n",
    "print X[:,1]\n",
    "\n",
    "for i in range(4):\n",
    "    print X[:,i].dot((sp.special.expit(-X.dot(w)) - y))\n"
   ]
